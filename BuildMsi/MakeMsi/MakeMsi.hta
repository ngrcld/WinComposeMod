;----------------------------------------------------------------------------
;--- I am not maintaining VERSION info in the ".VER" file -------------------
;----------------------------------------------------------------------------
<A HREF="http://dennisbareis.com/ppwizard/hashinclude.htm" TITLE="The #include command is used to include an external file (frequently called a &quot;Header File&quot;).&#10;&#10;A PPWIZARD command. Click for its internet based html documentation." TARGET="_blank" >#include</A> "MmVersion.mmh"         ;;Just include the MAKEMSI version #
<A HREF="http://dennisbareis.com/ppwizard/hashdefinerexx.htm" TITLE="Define some rexx code for immediate or deferred execution.&#10;&#10;A PPWIZARD command. Click for its internet based html documentation." TARGET="_blank" >#DefineRexx</A> ''
   ;--- Manipulate the MAKEMSI version number into a product version number ---
   @@Ver = '&lt;$MAKEMSI_VERSION&gt;'
   parse var @@Ver @@YY '.' +1 @@DDD +3 @@Fix
   @@YY  = right(@@YY,  2, '0');
   @@DDD = right(@@DDD, 3, '0');
   if  @@Fix = '' then
       @@Fix = '0'
   else
   do
       @@Fix = translate(@@Fix);
       if  pos(@@Fix, xrange('A', 'Z')) = 0 then
           <A HREF="http://dennisbareis.com/ppwizard/error.htm" TITLE="Abort as we have detected an error.&#10;&#10;A PPWIZARD based rexx function. Click for its internet based html documentation." TARGET="_blank" >error</A>('Invalid version fix letter of "' || @@Fix || '"');
       @@Fix = c2d(translate(@@Fix)) - c2d('A') + 1
   end;
   @@Fix = right(@@Fix, 4, '0');

   ;--- The "VER" file will refer to this -----------------------------------
   call <A HREF="http://dennisbareis.com/ppwizard/macroset.htm" TITLE="Define a symbol.&#10;&#10;A PPWIZARD based rexx function. Click for its internet based html documentation." TARGET="_blank" >MacroSet</A> 'HistoryVersion', @@YY || '.0.' || @@DDD || '.' || @@Fix
<A HREF="http://dennisbareis.com/ppwizard/hashdefinerexx.htm" TITLE="Define some rexx code for immediate or deferred execution.&#10;&#10;A PPWIZARD command. Click for its internet based html documentation." TARGET="_blank" >#DefineRexx</A>
<A HREF="http://dennisbareis.com/ppwizard/hashundef.htm" TITLE="Remove a symbol.&#10;&#10;A PPWIZARD command. Click for its internet based html documentation." TARGET="_blank" >#undef</A> MAKEMSI_VERSION


